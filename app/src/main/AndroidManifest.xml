<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shaunsheep.agsdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addgraphicsrenderer.AddGraphicsRenderer" />
        <activity android:name=".addgraphicssymbol.AddGraphicsSymbol" />
        <activity android:name=".arcgismapimagelayer.MapImageLayerUrl" />
        <activity android:name=".arcgistiledfromurl.TiledLayerFromUrl" />
        <activity android:name=".arcgisvectortiledlayerurl.VectorTiledlayerFromUrl" />
        <activity android:name=".blendrenderer.BlendRendererActivity" />
        <activity android:name=".changebasemap.ChangeBaseMapActivity" />
        <activity android:name=".changefeaturelayerrenderer.ChangeFeatureLayerRendererActivity" />
        <activity android:name=".changesublayervisible.ChangeSublayerVisibleActivity" />
        <activity android:name=".changeviewpoint.ChangeViewPointActivity" />
        <activity android:name=".colormaprenderer.ColormapRendererActivity" />
        <activity android:name=".creategeometry.CreateGeometriesActivity" />
        <activity android:name=".createsavemap.CreateSaveMapActivity" />
        <activity android:name=".createsavemap.MapSaveActivity"></activity>
        <activity android:name=".displaydevicelocation.DisplayDeviceLocationActivity"/>
        <activity android:name=".displaydrawingstatus.DisplayDrawingStatusActivity"/>
        <activity android:name=".displaylayerviewstate.DisplayLayerViewStateActivity"/>
        <activity android:name=".displaymap.DisplayMapActivity"/>
        <activity android:name=".editfeatureattachments.EditFeatureAttachActivity"/>
        <activity android:name=".editfeatureattachments.EditAttachmentActivity"/>
        <activity android:name=".featurelayerdefinitionexpression.FeatureLayerDefExpressionActivity"/>
        <activity android:name=".featurelayerfeatureservice.FeatureLayerServiceActivity"/>
        <activity android:name=".featurelayergeodatabase.FeatureLayerGeodatabaseActivity"/>
        <activity
            android:launchMode="singleTop"
            android:name=".featurelayerquery.FeatureLayerQueryActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <meta-data android:name="android.app.searchable" android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".featurelayerselection.FeatureLayerSelectionActivity"/>
        <activity android:name=".featurelayershowattributes.FeatureLayerShowAttrActivity"></activity>
        <activity android:name=".featurelayerupdateattributes.FeatureLayerUpAttrActivity"/>
        <activity android:name=".featurelayerupdateattributes.FLUPAttrDamageTypesListActivity"/>
        <activity android:name=".featurelayerupdategeometry.FeatureLayerUpGeoActivity"
            android:configChanges="orientation|screenSize|uiMode"/>
        <activity android:name=".findroute.FindRouteActivity"/>
        <activity android:name=".identifygraphicoverlay.IdentifyGraphicsOverlayActivity"
            android:configChanges="orientation|screenSize|uiMode"/>
        <activity android:name=".managebookmarks.ManagerBookmarksActivity"/>
        <activity android:name=".maploaded.MapLoadedActivity"/>
        <activity android:name=".manageoperationallayers.ManageOperationalLayerActivity"/>
        <activity android:name=".manageoperationallayers.OperationalLayers"/>
        <activity android:name=".maprotation.MapRotationActivity"/>
        <activity android:name=".mapsketching.MapSketchingActivity"/>
    </application>

</manifest>